{<br>
	CBI(LCD_SIG_PORT, LCD_RW);<br>
	SBI(LCD_SIG_PORT, LCD_RS);<br>
	SBI(LCD_SIG_PORT, LCD_E);<br>
	LCD_DATA_PORT =3D c;<br>
	_delay_us(43);<br>
	CBI(LCD_SIG_PORT, LCD_E);<br>
}</p>
